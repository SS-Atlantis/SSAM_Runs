cont_warn 0
K_num_soil_layers 0
K_num_catchments 0
K_max_num_occupations 0
K_max_active_cities 0
K_num_oil_companies 0

# Contaminant information (only used when track_contaminants set to 1)
num_contaminants  0

contaminant_names 0
Phenanthrene

contaminant_units 0
mgPAH/m^3

flag_pollutant_impacts 0

